{
  "name": "Memorable Information Rememberer",
  "description": "Autocomplete memorable information forms",
  "manifest_version": 2,
  "version": "0.1.0",

  "content_scripts": [{
    "js": ["autocomplete.js"],
    "matches": ["<all_urls>"]
  }],

  "options_ui": {
    "chrome_style": true,
    "page": "settings.html"
  },

  "permissions": [
    "<all_urls>",
    "storage"
  ]
}
